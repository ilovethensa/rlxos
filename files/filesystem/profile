# /etc/profle

umask 022

append_path() {
    case ":$PATH" in
        *:"$1":*)
            ;;
        *)
            PATH="${PATH:+$PATH:}$1"
    esac
}

append_path '/usr/bin'
export PATH

if test -d /etc/profile.d; then
    for profile in /etc/profile.d/*.sh; do
        test -r "$profile" && . "$profile"
    done
    unset profile
fi

unset -f append_path

unset TERMCAP
unset MANPATH